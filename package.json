{
  "name": "tycho-simulation-ts",
  "version": "0.1.0",
  "engines": {
    "node": ">=16.0.0"
  },
  "description": "TypeScript bindings for Tycho simulation",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "napi": {
    "name": "tycho-simulation-ts",
    "triples": {
      "defaults": true,
      "additional": [
        "aarch64-apple-darwin",
        "x86_64-apple-darwin",
        "x86_64-unknown-linux-gnu",
        "aarch64-unknown-linux-gnu"
      ]
    }
  },
  "binary": {
    "napi": {
      "name": "tycho-simulation-ts",
      "package": "tycho-simulation-ts",
      "triples": {
        "defaults": true,
        "additional": [
          "aarch64-apple-darwin",
          "x86_64-apple-darwin",
          "x86_64-unknown-linux-gnu",
          "aarch64-unknown-linux-gnu"
        ]
      }
    }
  },
  "files": [
    "dist",
    "dist/cjs",
    "index.node",
    "src",
    "binding.gyp",
    "README.md",
    "LICENSE",
    "tsconfig.json",
    "tsconfig.cjs.json",
    "build.rs",
    "Cargo.toml",
    "Cargo.lock"
  ],
  "scripts": {
    "artifacts": "napi artifacts",
    "build:rust": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "clean": "rm -rf target index.node dist 2>/dev/null || true",
    "prepublishOnly": "npm run build",
    "prepare": "npm run build",
    "prepack": "npm run build && napi prepublish -t npm",
    "test": "jest",
    "universal": "napi universal",
    "version": "napi version",
    "build:ts": "tsc && tsc -p tsconfig.cjs.json",
    "build": "npm run build:rust && npm run build:ts",
    "test:amount": "npm run build && ts-node examples/amount_out_test.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dewiz-xyz/tycho-simulation-ts.git"
  },
  "keywords": [
    "tycho",
    "simulation",
    "defi",
    "blockchain",
    "napi-rs",
    "node-api"
  ],
  "author": "Dewiz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dewiz-xyz/tycho-simulation-ts/issues"
  },
  "homepage": "https://github.com/dewiz-xyz/tycho-simulation-ts#readme",
  "dependencies": {
    "@napi-rs/cli": "^2.18.0",
    "typescript": "^4.5.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^16.18.125",
    "dotenv": "^16.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2"
  }
}
